<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.etlpat.mapper.BankMapper">

    <resultMap id="BaseResultMap" type="com.etlpat.pojo.Bank">
            <id property="bankId" column="bank_id" />
            <result property="bankName" column="bank_name" />
            <result property="introduce" column="introduce" />
            <result property="bankPhone" column="bank_phone" />
            <result property="money" column="money" />
            <result property="rate" column="rate" />
            <result property="repayment" column="repayment" />
    </resultMap>

    <sql id="Base_Column_List">
        bank_id,bank_name,introduce,bank_phone,money,rate,
        repayment
    </sql>
</mapper>
